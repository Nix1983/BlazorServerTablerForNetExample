@page "/"
@using Microsoft.AspNetCore.Components.Web
@namespace BlazorServerTablerForNetExample.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <base href="~/" />
    <!--Preloading resources-->
    <link href="css/site.css" rel="preload" as="style" />
    <link href="_content/TablerForNet/css/tablerForNet.min.css" rel="preload" as="style" />

    <!--CSS-->
    <link href="css/" rel="stylesheet" />
    <link href="_content/TablerForNet/css/tablerForNet.min.css" rel="stylesheet" />

    <link href="icons/logo.svg" rel="icon" type="image/png" />
    <link href="icons/logo.svg" rel="apple-touch-icon" sizes="any">
    <component type="typeof(HeadOutlet)" render-mode="ServerPrerendered" />
</head>
<body>
    <component type="typeof(App)" render-mode="ServerPrerendered" />

    <div id="blazor-error-ui">
        <environment include="Staging,Production">
            An error has occurred. This application may no longer respond until reloaded.
        </environment>
        <environment include="Development">
            An unhandled exception has occurred. See browser dev tools for details.
        </environment>
        <a href="@("/error")" class="reload">Reload</a>
        <a href="@("/error")" class="dismiss">🗙</a>
    </div>

    <script src="_content/TablerForNet/js/tablerForNet.min.js"></script>
    <script src="_framework/blazor.server.js"></script>
</body>
</html>
